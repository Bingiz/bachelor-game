      answercount: 0
      DialogueEvent:
        m_PersistentCalls:
          m_Calls: []
    - name: '#Kairosit'
      topic: {fileID: 11400000, guid: 9174b84a5d77848079a34d30fc1ee9ad, type: 2}
      answers:
      - "Von diesem Stein verstehe ich nichts. Frag lieber Adelaide. Sie hat sich
