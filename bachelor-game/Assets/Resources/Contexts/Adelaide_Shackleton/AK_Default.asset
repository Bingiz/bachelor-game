        fragil."
      answercount: 0
      DialogueEvent:
        m_PersistentCalls:
          m_Calls: []
    - name: '#Friedhof'
      topic: {fileID: 11400000, guid: 05f3b8a74bddc4cb79550cdef915a06c, type: 2}
