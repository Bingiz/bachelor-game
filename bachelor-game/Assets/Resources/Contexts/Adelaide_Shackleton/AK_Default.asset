        m_PersistentCalls:
          m_Calls: []
    - name: "#R\xFCckreise"
      topic: {fileID: 11400000, guid: aa485dd192adb47c180002087e9e0e61, type: 2}
      answers:
      - "Wir k\xF6nnen fr\xFChestens in vier Monaten zur\xFCckkehren. Nach dem Winter.
